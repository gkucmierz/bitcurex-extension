{
  "manifest_version": 2,

  "name": "bitcurex-extension",
  "description": "bitcurex.com extension",
  "version": "1.0",
  
  "content_scripts": [
    {
      // Change 'matches' attribute to load content
      // script only in pages you want to.
      "matches": ["https://*.bitcurex.com/"],
      "js": ["market.js"]
    }
  ]
}